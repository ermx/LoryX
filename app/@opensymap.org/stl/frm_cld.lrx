        loryx.org/value % 04Rfvkqs89
select c.*
from [@loryx] c
left join [@loryx] ooo on (c.l=ooo.l and c.o=ooo.o and c.r=ooo.r and ooo.y='loryx.org/ord')
where c.y = 'loryx.org/type'
and c.l=[#hdn_sys]
and c.o=[#hdn_base]
and c.r=[#txt_name]
04Rfvkqs89
