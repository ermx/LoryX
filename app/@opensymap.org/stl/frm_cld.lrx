        [ q1 opensymap.org/db/query
            loryx.org/value % cACteg0TeK
select a.*
from [@loryx] a
where a.l=[#hdn_sys]
and a.o = [#hdn_base]
and a.r = [#txt_name]
cACteg0TeK
